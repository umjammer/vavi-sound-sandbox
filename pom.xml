<?xml version="1.0" encoding="UTF-8"?>

<project
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>vavi</groupId>
  <artifactId>vavi-sound-sandbox</artifactId>
  <version>1.0.2</version>

  <name>Vavi Sound Sandbox API</name>
  <organization>
    <name></name>
  </organization>
  <url>https://github.com/umjammer/vavi-sound-sandbox</url>
  <description>1.0.2

    alac spi
    opus spi

    itunes library (jna)

1.0.1

    maintenance version

TODO

    jni in maven
    resampling spi
    qt has been deprecated
  </description>
  <scm>
    <url>https://github.com/umjammer/vavi-sound-sandbox</url>
  </scm>
  <issueManagement>
    <url>https://github.com/umjammer/vavi-sound-sandbox/issues</url>
  </issueManagement>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>8</source>
          <target>8</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <repositories>
    <repository>
      <id>umjammer-mvn-repo</id>
      <url>https://raw.github.com/umjammer/umjammer/mvn-repo/</url>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </snapshots>
    </repository>
    <repository>
      <id>jitpack.io</id>
      <url>https://jitpack.io</url>
    </repository>
  </repositories>

  <dependencies>
    <dependency>
      <groupId>com.github.umjammer</groupId>
      <artifactId>vavi-sound</artifactId>
      <version>1.0.8</version>
    </dependency>
    <dependency>
      <groupId>com.github.umjammer</groupId>
      <artifactId>Java-Apple-Lossless-decoder</artifactId>
      <version>0.2.9</version>
    </dependency>
    <dependency>
      <groupId>org.rococoa</groupId>
      <artifactId>rococoa-cocoa</artifactId>
      <version>0.5</version>
    </dependency>
    <dependency>
      <groupId>com.github.umjammer</groupId>
      <artifactId>vavi-commons-sandbox</artifactId>
      <version>0.3.5</version>
    </dependency>
    <dependency>
      <groupId>com.github.umjammer.tritonus</groupId>
      <artifactId>tritonus-remaining</artifactId>
      <version>0.3.7-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>com.github.lostromb</groupId>
      <artifactId>concentus</artifactId>
      <version>fdf276e</version>
      <type>pom</type>
    </dependency>

    <dependency>
      <groupId>javax.media</groupId>
      <artifactId>jmf</artifactId>
      <version>2.1.1e</version>
    </dependency>

    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <version>5.3.0</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-engine</artifactId>
      <version>5.3.0</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>urbanophile</groupId>
      <artifactId>java-getopt</artifactId>
      <version>1.0.9</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>com.github.umjammer.tritonus</groupId>
      <artifactId>tritonus-cdda</artifactId>
      <version>0.3.7-SNAPSHOT</version>
      <scope>test</scope>
    </dependency>
